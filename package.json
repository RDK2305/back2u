{
  "name": "Back2u",
  "version": "1.0.0",
  "description": "Back2u - Lost & Found System API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "node build-css.js",
    "watch:css": "nodemon --exec \"node build-css.js\" --watch public --watch tailwind.config.js --watch postcss.config.js",
    "setup:db": "node config/setupDatabase.js",
    "team:list": "node utils/team-config-util.js list",
    "team:info": "node utils/team-config-util.js info",
    "team:branches": "node utils/team-config-util.js branches"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "nodemon": "^2.0.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
